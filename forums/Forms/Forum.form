<item name='$$FormScript' summary='false' sign='true'><text>'++LotusScript Development Environment:2:5:(Options):0:66

'++LotusScript Development Environment:2:5:(Forward):0:1
Declare Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
Declare Sub Postopen(Source As Notesuidocument)
Declare Sub Postsave(Source As Notesuidocument)

'++LotusScript Development Environment:2:5:(Declarations):0:2

'++LotusScript Development Environment:2:2:BindEvents:1:129
Private Sub BindEvents(Byval Objectname_ As String)
	Static Source As NOTESUIDOCUMENT
	Set Source = Bind(Objectname_)
	On Event Queryopen From Source Call Queryopen
	On Event Postopen From Source Call Postopen
	On Event Postsave From Source Call Postsave
End Sub

'++LotusScript Development Environment:2:2:Queryopen:1:12
Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
	
End Sub
'++LotusScript Development Environment:2:2:Postopen:1:12
Sub Postopen(Source As Notesuidocument)
	readersValue = Source.FieldGetText("Readers")
End Sub
'++LotusScript Development Environment:2:2:Postsave:1:12
Sub Postsave(Source As Notesuidocument)
	If Source.FieldGetText("Readers") &lt;&gt; readersValue Then
		Dim entries As NotesViewEntryCollection
		
		Dim topics As NotesView
		Set topics = Source.Document.ParentDatabase.GetView("Topics")
		topics.AutoUpdate = False
		Call topics.ResortView("ForumID", True)
		
		Set entries = topics.GetAllEntriesByKey(Source.Document.ForumID(0), True)
		Call entries.StampAll("ReadersFailsafe", Source.Document.ReadersFailsafe)
		Call entries.StampAll("Readers", Source.Document.Readers)
		
		Dim posts As NotesView
		Set posts = Source.Document.ParentDatabase.GetView("Posts")
		posts.AutoUpdate = False
		Call posts.ResortView("ForumID", True)
		
		Set entries = posts.GetAllEntriesByKey(Source.Document.ForumID(0), True)
		Call entries.StampAll("ReadersFailsafe", Source.Document.ReadersFailsafe)
		Call entries.StampAll("Readers", Source.Document.Readers)
		
		readersValue = Source.FieldGetText("Readers")
	End If
End Sub<break/></text></item>
